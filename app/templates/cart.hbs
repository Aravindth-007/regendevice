{{outlet}}

<GeneralContainer />
<div class="cart-container">
  {{#each this.cart.itemList as |item|}}
  <div class="add-cart-price-content">
  <div class="product-card">
  <img class="product-image" src="{{item.image}}" alt="Product Image">
  <div class="product-details">
    <div class="product-name">{{item.model}}</div>
    <div class="product-price">{{item.price}}</div>
  </div>
</div>
<div class="price-div">
  <div class="row">
      <span class="col">Subtotal : {{item.price}}</span> 
    </div>
    <div class="row">
      <span class="col">Tax : {{this.tax}}</span>
    </div>
</div>
</div>
  {{/each}}

